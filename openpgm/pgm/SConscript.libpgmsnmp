# -*- mode: python -*-
# OpenPGM build script
# $Id$

Import('env')

e = env.Clone()
e.Append(CCFLAGS = '-DG_LOG_DOMAIN=\'"Pgm-Snmp"\'');

e.MergeFlags(e['SNMP_FLAGS']);

src = Split("""
		snmp.c
		pgmMIB.c
""")

e.StaticLibrary('libpgmsnmp', src);
e.StaticSharedLibrary('libpgmsnmp-pic', src);

# end of file
